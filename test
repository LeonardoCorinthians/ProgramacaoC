#include <stdio.h>

int main() {

  char estado1;
  char estado2;
  char carta1[50];
  char carta2[50];
  char cidade1[50];
  char cidade2[50];
  int populacao1;
  int populacao2;
  float areakm1;
  float areakm2;
  float pib1;
  float pib2;
  int pontos1;
  int pontos2;


  ////Dados da cidade da carta 1////


  printf("Digite a inicial do estado da carta 1: \n");
  scanf(" %c", &estado1);
 printf("Estado: %c\n", estado1);

  printf("Digite o código da carta 1: \n");
  scanf("%s", carta1);
printf("Codigo da carta: %s\n", carta1);

  printf("Digite o nome da cidade da carta 1: \n");
  scanf("%s", cidade1);
printf("Populacao: %d\n", populacao1);

  printf("Digite a populacao da cidade da carta 1: \n");
  scanf("%d", &populacao1);
printf("Pontos turisticos: %d\n", populacao1);

  printf("Digite a area em km² da cidade da carta 1: \n");
  scanf("%.2f", &areakm1);
 printf("Area em km²: %.2f\n", areakm1);

  printf("Digite o pib da cidade da carta 1: \n");
  scanf("%.2f", &pib1);
printf("PIB da cidade: %.2f\n", pib1);

  printf("Digite o numero de pontos turisticos na cidade da carta 1: \n");
  scanf("%d", &pontos1);
  printf("Pontos turisticos: %d\n", pontos1);




  ////Dados da cidade da carta 2////


  printf("Digite a inicial do estado da carta 2: \n");
  scanf(" %c", &estado2);
 printf("Estado: %c\n", estado2);

  printf("Digite o código da carta 2: \n");
  scanf("%s", carta2);
printf("Codigo da carta: %s\n", carta2);

  printf("Digite o nome da cidade da carta 2: \n");
  scanf("%s", cidade2);
printf("Populacao: %d\n", populacao2);

  printf("Digite a populacao da cidade da carta 2: \n");
  scanf("%d", &populacao2);
printf("Pontos turisticos: %d\n", populacao2);

  printf("Digite a area em km² da cidade da carta 2: \n");
  scanf("%.2f", &areakm2);
 printf("Area em km²: %.2f\n", areakm2);

  printf("Digite o pib da cidade da carta2: \n");
  scanf("%.2f", &pib2);
printf("PIB da cidade: %.2f\n", pib2);

  printf("Digite o numero de pontos turisticos na cidade da carta 2: \n");
  scanf("%d", &pontos2);
  printf("Pontos turisticos: %d\n", pontos2);


  return 0;


}